# Custom exports

# Don't ever forget this !
export PATH="/usr/local/sbin:/usr/local/bin:/usr/bin:/usr/lib/jvm/default/bin:/usr/bin/site_perl:/usr/bin/vendor_perl:/usr/bin/core_perl"

# Adding .myscripts folder to path
export PATH=$HOME/.myscripts:$PATH

# Adding path to dart-sdk
export PATH=$HOME/dartium:$PATH

export PATH=/opt/dart-sdk/bin/:$PATH

# Adding path for arcanist to allow arc command
export PATH=/home/saksham/arcanist/bin:$PATH    

export PATH=/home/saksham/.pub-cache/bin:$PATH

export PATH=/home/saksham/.gem/ruby/2.2.0/bin:$PATH

export PATH=/home/saksham/.cabal/bin:$PATH

export PATH=/home/saksham/kubernetes/cluster/ubuntu/binaries:$PATH

# Node version manager
export NVM_DIR="/home/saksham/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm

export EDITOR=vim

export MYZSH=/home/saksham/.myscripts/zsh

export LFS=/home/saksham/mnt/LFS/

export GOPATH=/home/saksham/gocode
